# Appwrite Configuration
# Copy this file to .env and fill in your actual values

# Appwrite Project ID
VITE_APPWRITE_PROJECT_ID=your_project_id_here

# Appwrite Endpoint URL
VITE_APPWRITE_ENDPOINT=https://your-appwrite-instance.com/v1

# Appwrite Database ID
VITE_APPWRITE_DATABASE_ID=your_database_id_here

# Appwrite Collection IDs
VITE_APPWRITE_DATABASE_ENQUIRY_ID=your_enquiry_collection_id_here
VITE_APPWRITE_DATABASE_APPLICATION_ID=your_application_collection_id_here

# Note: All variables must start with VITE_ to be accessible in the frontend
# Never commit your actual .env file to version control
